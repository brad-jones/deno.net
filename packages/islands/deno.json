{
  "name": "@brad-jones/deno-net-islands",
  "exports": {
    "./client": "./src/client.tsx",
    "./server": "./src/server.ts"
  },
  "version": "0.1.1",
  "license": "MIT",
  "publish": {
    "include": [
      "src/**/*.ts",
